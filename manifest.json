{
  "manifest_version": 2,
  "name": "Intilery Quick Launcher",
  "description": "Quick launch links",
  "version": "1.0.0",
  "icons": {
    "128": "/imgs/icon_128.png"
  },
  "browser_action": {
    "default_icon": "/imgs/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}